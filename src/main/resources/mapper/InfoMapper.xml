<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dao.InfoMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.entity.Info">
        <id column="id" property="id" />
        <result column="pic_name" property="picName" />
        <result column="pic_local_path" property="picLocalPath" />
        <result column="pic_local_md" property="picLocalMd" />
        <result column="pic_url" property="picUrl" />
        <result column="pic_url_md" property="picUrlMd" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, pic_name, pic_local_path, pic_local_md, pic_url, pic_url_md
    </sql>

</mapper>
